<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--colors--> 
    <Color x:Key="WhiteColor">#f0f5f7</Color>
    <Color x:Key="BlueColor">#69929f</Color>
    <Color x:Key="LightBlueColor">#9fbac2</Color>
    <Color x:Key="UltraLightBlueColor">#d5e1e6</Color>
    
    <!--brushes-->
    <SolidColorBrush x:Key="WhiteColorBrush" Color="{DynamicResource WhiteColor}" />
    <SolidColorBrush x:Key="BlueColorBrush" Color="{DynamicResource BlueColor}" />
    <SolidColorBrush x:Key="LightBlueColorBrush" Color="{DynamicResource LightBlueColor}" />
    <SolidColorBrush x:Key="UltraLightBlueColorBrush" Color="{DynamicResource UltraLightBlueColor}" />

</ResourceDictionary>